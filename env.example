# ACR 설정 (예시 값, 실제 값은 로컬 env/.env.{env}에 저장)
ACR_LOGIN_SERVER=<acr_login_server>
ACR_REPO_NAME_BACKEND=<backend_repo_name>
ACR_REPO_NAME_FRONTEND=<frontend_repo_name>

# Kubernetes 설정
K8S_NAMESPACE=<namespace>

MYSQL_HOST=<mariadb_release_name>
MYSQL_USER=<db_user>
MYSQL_PASSWORD=<db_password>
MYSQL_DBNAME=<db_name>

# Redis 비고: REDIS_HOST는 기본 릴리스 이름(예: yejun-redis)만 입력합니다.
# 백엔드 매니페스트에서 자동으로 -master를 붙여 사용합니다.
REDIS_HOST=<redis_release_name>
REDIS_PASSWORD=<redis_password>

KAFKA_SERVERS=<kafka_release_name>
KAFKA_USERNAME=<kafka_username>
KAFKA_PASSWORD=<kafka_password>

FLASK_SECRET_KEY=<flask_secret_key>

DOCKER_HUB_USERNAME=<docker_hub_username>